<?xml version="1.0"?>
<inspection xmlns="http://schemas.xmlsoap.org/ws/2001/10/inspection/">
    
    <service>
        <name>TravelGood</name>
        <abstract>Travel Planning and Booking service from TravelGood</abstract>
        <description referencedNamespace="http://schemas.xmlsoap.org/wsdl/"
                     location="http://localhost:9080/TravelGood/service?wsdl"/>
    </service>
    
    <link referencedNamespace="http://schemas.xmlsoap.org/ws/2001/10/inspection/" 
          location="http://localhost:8080/NiceViewWSIL/inspection.wsil" >
        <!--the path to NiceView WSIL can be in any other location in the glassfish server-->
        <abstract>Hotel registration company</abstract>
    </link>    

    <link referencedNamespace="http://schemas.xmlsoap.org/ws/2001/10/inspection/" 
              location="http://localhost:8080/LameDuckWSIL/inspection.wsil" >
        <!--the path to LameDuck WSIL can be in any other location in the glassfish server-->        
        <abstract>Travel planning company</abstract>
    </link>	
</inspection>

